pipeline {    
    options { skipDefaultCheckout() }
     agent {label 'master'} 
     environment {PATH ="/usr/bin/mvn:$PATH"}
      
    stages { 
     stage('SCM') {
            steps {
               
                git  branch: 'main', url: 'https://github.com/AhmedRomdhani16/PROJECT-CI.git'
                 
            }
        }
     stage('build'){
            steps {
            sh "mvn clean install -X"
            }
            }
}
}
